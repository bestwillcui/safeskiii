>\n    <form id=\"sign-up\" class='component' v-on:submit.prevent=\"signUp\" method=\"post\">\n      <input id='username' v-model.trim='username' type='text' name='username' placeholder=\"Username\">\n      <input id='password' v-model.trim='password' type='password' name='password' placeholder=\"Password\">\n      <br/>\n      <input id='code' v-model.trim='code' type='text' name='code' placeholder=\"Code\">\n      <br/>\n      <input type='submit' value='Create Account' class=\"button\">\n    </form>\n    <div v-if='errors.length' class=\"error-message\" style=\"width: 250px;\">\n      <b>Please correct the following error(s):</b>\n      <ul>\n        <li v-for='error in errors' v-bind:key='error.id'>{{ error }}</li>\n      </ul>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { eventBus } from \"../main\";\n\nexport default {\n  name: \"SignUp\",\n\n  data() {\n    return {\n      errors: [],\n      username: \"\",\n      password: \"\",\n      code: \"\"\n    }\n  },\n\n  methods: {\n    signUp: function() {\n      const bodyContent = { username: this.username, password: this.password, code: this.code };\n        axios\n          .post(\"/api/users/\", bodyContent)\n          .then(() => {\n            // handle success\n            eventBus.$emit('signup-success', true);\n          })\n          .catch(err => {\n            // handle error\n            this.errors.push(err.response.data.message);\n          })\n          .then(() => {\n            // always executed\n            this.resetForm();\n            this.clearMessages();\n          });\n    },\n\n    resetForm: function() {\n      this.username = \"\";\n      this.password = \"\";\n    },\n\n    clearMessages: function() {\n      setInterval(() => {\n        this.errors = [];\n      }, 5000);\n    },\n  }\n}\n</script>\n\n<style scoped>\nform {\n  width: fit-content;\n  position: relative;\n}\n\ninput[type=\"text\"],\ninput[type=\"password\"],\ninput[type=\"url\"] {\n  width: 20rem;\n  font-size: 20px;\n  padding: 6px 12px;\n  margin: 2px 0;\n  box-sizing: border-box;\n  font-family: \"Red Hat Text\";\n}\n\n.component {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.button {\n  color: white;\n  font-size: 18px;\n  font-weight: 700;\n  \n  cursor: pointer;\n  background-color: #006494;\n  filter: drop-shadow(0px 4px 4px rgba(0, 0, 0, 0.25));\n  border: transparent;\n  border-radius: 15px;\n  padding: 16px 32px;\n  width: 256px;\n  text-align: center;\n  font-family: \"Red Hat Text\";\n}\n\n.button:hover {\n  opacity: 0.8;\n  color: white;\n}\n</style>"],"sourceRoot":"src/components"}]}