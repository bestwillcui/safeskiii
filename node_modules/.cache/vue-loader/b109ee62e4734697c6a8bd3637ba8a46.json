-on:click=\"deleteAccount\" class=\"change\">Delete Account</button>\n    <button v-on:click=\"signOut\" class=\"change\">Sign Out</button>\n    <div v-if='errors.length' class=\"error-message\" style=\"width: 250px;\">\n      <b>Please correct the following error(s):</b>\n      <ul>\n        <li v-for='error in errors' v-bind:key='error.id'>{{ error }}</li>\n      </ul>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { eventBus } from \"../main\";\n\nexport default {\n  name: \"Settings\",\n  \n  data() {\n    return {\n      errors: [],\n      newUsername: \"\",\n      newPassword: \"\"\n    }\n  },\n\n  methods: {\n    changeUsername: function() {\n      axios\n        .put(`/api/users/${this.newUsername}/username`)\n        .then(() => {\n          // handle success\n          eventBus.$emit('username-change-success', { username: this.newUsername });\n        })\n        .catch(err => {\n          // handle error\n          this.errors.push(err.response.data.error);\n        })\n        .then(() => {\n          // always executed\n          this.resetUsernameForm();\n        });\n    },\n\n    changePassword: function() {\n      const bodyContent = { password: this.newPassword };\n      axios\n        .put(\"/api/users/password\", bodyContent)\n        .then(() => {\n          // handle success\n          eventBus.$emit('password-change-success', bodyContent);\n        })\n        .catch(err => {\n          // handle error\n          this.errors.push(err.response.data.error);\n        })\n        .then(() => {\n          // always executed\n          this.resetPasswordForm();\n        });\n    },\n\n    signOut: function() {\n      axios.delete('/api/users/signout')\n        .then(() => {\n          // handle success\n          eventBus.$emit('signout-success', true);\n        })\n        .catch(() => {\n          // Still sign User out so they have to sign in again.\n          eventBus.$emit('signout-success', true);\n        })\n    },\n\n    deleteAccount: function() {\n      axios.delete('/api/users')\n        .then(() => {\n          // handle success\n          eventBus.$emit('delete-success', true);\n        })\n        .catch(() => {\n          // Sign User out so they have to sign in again.\n          eventBus.$emit('signout-success', true);\n        })\n    },\n\n    resetUsernameForm: function() {\n      this.newUsername = \"\";\n    },\n\n    resetPasswordForm: function() {\n      this.newPassword = \"\";\n    },\n  }\n}\n</script>\n\n<style scoped>\n.settings {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.change {\n  color: white;\n  margin: 2rem 0;\n  font-size: 14px;\n  font-weight: 700;\n  \n  cursor: pointer;\n  background-color: #000000;\n  filter: drop-shadow(0px 4px 4px rgba(0, 0, 0, 0.25));\n  border: transparent;\n  border-radius: 15px;\n  padding: 8px 16px;\n  width: 200px;\n  text-align: center;\n}\n\n.change:hover {\n    opacity: 0.8;\n    color: white;\n}\n</style>\n"]}]}